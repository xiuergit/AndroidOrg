<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BaseActivity$WebViewActivity">

    <Button
        android:id="@+id/bt_mycv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="我的简历"
        android:onClick="opencv"
        />


    <Button
        android:id="@+id/bt_h5"
        android:layout_toRightOf="@id/bt_mycv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="h5结构的学习"
        android:onClick="study"
        />

    <com.bigdata.webshell.uiview.ZXWebView
        android:id="@+id/zx_webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/bt_mycv">

    </com.bigdata.webshell.uiview.ZXWebView>

</RelativeLayout>
